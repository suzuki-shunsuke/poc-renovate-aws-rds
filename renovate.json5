{
  extends: ["config:base"],
  regexManagers: [
    {
      fileMatch: ["\\.tf$"],
      matchStrings: [
        '"(?<currentValue>.*?)" # renovate: depName=(?<depName>.*?)',
      ],
      datasourceTemplate: "github-tags",
      versioningTemplate: "docker", // RDS engine version is invalid semver
    },
  ],
}
